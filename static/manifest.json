{
   "content_scripts": [ {
      "js": ["bundle.min.js"],
      "css": ["style.css"],
      "matches": [
         "https://github.com/*/pull/*",
         "https://github.com/*/commit/*"
      ]
   } ],
   "description": "Adds autocomplete functionality, sourced from the diff, when reviewing GitHub commits and pull requests.",
   "homepage_url": "https://github.com",
   "manifest_version": 2,
   "minimum_chrome_version": "20.0",
   "name": "AutoComplete for GitHub",
   "permissions": [],
   "version": "0.61"
}
