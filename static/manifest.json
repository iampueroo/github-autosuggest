{
   "content_scripts": [ {
      "js": ["bundle.min.js"],
      "css": ["style.css"],
      "matches": [
         "https://github.com/*/pull/*",
         "https://github.com/*/commit/*",
         "https://gitlab.com/*/commit/*",
         "https://gitlab.com/*/merge_requests/*"
      ]
   } ],
   "icons": {
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
   },
   "description": "Adds autocomplete functionality, sourced from the diff, when reviewing GitHub commits and pull requests.",
   "homepage_url": "https://github.com/iampueroo/github-autosuggest",
   "manifest_version": 2,
   "minimum_chrome_version": "20.0",
   "name": "GitHub Code Review Autocomplete",
   "permissions": [],
   "version": "0.65"
}
